\newcommand{\vlgwnlog}{Gross Wealth (log)}                   % gw_nlog
\newcommand{\vlnwnlog}{Net Wealth (neglog)}                  % nw_nlog
\newcommand{\vlgw}{Gross Wealth (k€, winsored)}              % gw
\newcommand{\vlnw}{Net Wealth (k€, winsored)}                % nw
\newcommand{\vlexpue}{Unemployment exp. (months)}            % expue
\newcommand{\vlexpft}{Full-Time exp. (months)}               % expft
\newcommand{\vlemplstatus}{Employment Status of Individual}  % empl_status
\newcommand{\vllaborearns}{Individual Labor Earnings}        % labor_earns
\newcommand{\vlcardioev}{Cardiopathy}                        % cardio_ev
\newcommand{\vldepresev}{Depression}                         % depres_ev


% -------------------------------------------------------------------------------------------------
%  {\vlgwnlog}{Gross Wealth (log)}                   % gw_nlog
% -------------------------------------------------------------------------------------------------
% -------------------------------------------------------------------------------------------------
\begin{figure}[htb!]
    \centering
    \begin{subfigure}{0.45\textwidth}
        \caption{Gross Wealth (log); Models: MP1 / MM1}
        \includegraphics[width=.95\linewidth]{../../output/figures/csdid2/f_robust/ep1_gw_nlog.pdf}
        \label{sfig:ep1_gw_nlog}
    \end{subfigure}
    \begin{subfigure}{0.45\textwidth}
        \caption{Gross Wealth (log); Models: MP2 / MM2}
        \includegraphics[width=.95\linewidth]{../../output/figures/csdid2/f_robust/ep2_gw_nlog.pdf}
        \label{sfig:ep2_gw_nlog}
    \end{subfigure}
    \begin{subfigure}{0.45\textwidth}
        \caption{Gross Wealth (log); Models: MP3 / MM3}
        \includegraphics[width=.95\linewidth]{../../output/figures/csdid2/f_robust/ep3_gw_nlog.pdf}
        \label{sfig:ep3_gw_nlog}
    \end{subfigure}
    \begin{subfigure}{0.45\textwidth}
        \caption{Gross Wealth (log); Models: MP4 / MM4}
        \includegraphics[width=.95\linewidth]{../../output/figures/csdid2/f_robust/ep4_gw_nlog.pdf}
        \label{sfig:ep4_gw_nlog}
    \end{subfigure}
    \caption{Varying model specification (Response var: \vlgwnlog)}
    \par \footnotesize \raggedright
    \vspace{5pt} 
    Notes: Table of results corresponding to this figure in \cref{tbl:t_cmp_gw_nlog}.
    \label{fig:ep_gw_nlog}
\end{figure}

% -------------------------------------------------------------------------------------------------
\begin{table}[htb!]
    \begin{adjustbox}{max width=\textwidth}
        \begin{threeparttable}
            \caption{Varying model specification (Response var: \vlgwnlog)}
            \label{tbl:t_cmp_gw_nlog}
            \begin{tabular}{l*{8}{c}}
                \toprule
                & \multicolumn{4}{c}{Physical Health} & \multicolumn{4}{c}{Mental Health}                             \\ \cmidrule(l){2-5} \cmidrule(l){6-9} 
                & (MP1)       & (MP2)       & (MP3)        & (MP4)        & (MM1)       & (MM2)       & (MM3)        & (MM4)       \\
                \midrule%
                \input{./incl/tbls/t_cmp_gw_nlog.tex}
            \end{tabular}
            \begin{tablenotes}[para,flushleft]
                \vspace*{-\baselineskip}
                {\raggedleft*~$p<0.05$,~**~$p<0.01$,~***~$p<0.001$\\}
                Notes: For this robustness checks, the same model with varying specification was run to check stability of 
                estimated coefficients. In all models the logarithm of Gross Wealth is the response variable. 
                Models MP1 and MM1 is the same as the models in the main results. Models MP2 and MM2 do 
                not include any covariates. Models MP3 and MM3 include covariates and are weighted 
                by the inverse probability of remaining in the SOEP, to account for sample attrition which might
                impact the wealth distribution differentially. Finally, models MP4 and MM4 are balanced,
                in the sense that only people that are present from 2002 until 2020 are kept this specification. 
                The coefficients are stable across models. The estimated coefficients of the mental health domain 
                are consistently higher than those of the physical domain. 
                Notes: Figure corresponding to this table in \cref{tbl:t_cmp_gw_nlog}. \\
                
            \end{tablenotes}
        \end{threeparttable}
    \end{adjustbox}
\end{table}
